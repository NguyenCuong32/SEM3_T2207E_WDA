@model IEnumerable<demo.Models.Project>

@{
    ViewData["Title"] = "List Project";
}


<p>
    <a class="btn btn-primary" asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead class="thead-dark">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ProjectKey)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProjectName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProjectLead)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.UserCrate)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ProjectKey)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProjectName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProjectLead)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserCrate)
            </td>
            <td>
                <a class="btn btn-info" asp-action="Edit" asp-route-id="@item.ProjectId">Edit</a> |
                <a class="btn btn-warning" asp-action="Details" asp-route-id="@item.ProjectId">Details</a> |
                <a class="btn btn-danger" asp-action="Delete" asp-route-id="@item.ProjectId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
