<!-- Section support -->
<div id="body">
	<div id="chat-circle" class="btn btn-raised">
		<div id="chat-overlay"></div>
		<i class="bi bi-chat fs-2"></i>
	</div>

	<div class="chat-box">
		<div class="chat-box-header">
			<div class="w-100 d-flex">
				<div class="chat-msg-online" style="">
					<div class="msg-avatar"><img src="/images/girl-card.jpeg"></div>
					<div class="online-dot"></div>
				</div>
				<div class="ms-2">
					<div class="fw-bold text-start" style="font-size: 16px;">Angela Phương Trinh</div>
					<div class="text-white-50 text-start" style="font-size: 16px;">Online</div>
				</div>
			</div>
			<span class="chat-box-toggle"><i class="bi bi-x"></i></span>
		</div>
		<div class="chat-box-body">
			<div class="chat-box-overlay">
			</div>
			<div class="chat-logs">

			</div><!--chat-log -->
		</div>
		<div class="chat-input">
			<form>
				<input type="text" id="chat-input" placeholder="Send a message..." />
				<button type="submit" class="chat-submit" id="chat-submit"><i class="bi bi-send"
						style="color: var(--blue-color);"></i></button>
			</form>
		</div>
	</div>
</div>